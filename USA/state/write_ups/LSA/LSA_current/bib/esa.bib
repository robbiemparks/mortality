@article{Luterbacher2004,
Author = {Luterbacher, J and Dietrich, D and Xoplaki, E and Grosjean, M and
   Wanner, H},
Title = {{European seasonal and annual temperature variability, trends, and
   extremes since 1500}},
Journal = {{Science}},
Year = {{2004}},
Volume = {{303}},
Number = {{5663}},
Pages = {{1499-1503}},
Month = {{Mar 5}},
Abstract = {{Multiproxy reconstructions of monthly and seasonal surface temperature
   fields for Europe back to 1500 show that the late 20th- and early
   21st-century European climate is very likely (>95\% confidence level)
   warmer than that of any time during the past 500 years. This agrees with
   findings for the entire Northern Hemisphere. European winter average
   temperatures during the period 1500 to 1900 were reduced by similar
   to0.5degreesC (0.25degreesC for annual mean temperatures) compared to
   the 20th century. Summer temperatures did not experience systematic
   century-scale cooling relative to present conditions. The coldest
   European winter was 1708/1709; 2003 was by far the hottest summer.}},
Publisher = {{AMER ASSOC ADVANCEMENT SCIENCE}},
Address = {{1200 NEW YORK AVE, NW, WASHINGTON, DC 20005 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Luterbacher, J (Reprint Author), Univ Bern, NCCR Climate, CH-3012 Bern, Switzerland.
