@article{Fisman2007,
Author = {Fisman, David N.},
Title = {{Seasonality of infectious diseases}},
Journal = {{ANNUAL REVIEW OF PUBLIC HEALTH}},
Series = {{Annual Review of Public Health}},
Year = {{2007}},
Volume = {{28}},
Pages = {{127-143}},
Abstract = {{Seasonality, a periodic surge in disease incidence corresponding to
   seasons or other calendar periods, characterizes many infectious
   diseases of public health importance. The recognition of seasonal
   patterns in infectious disease occurrence dates back at least as far as
   the Hippocratic era, but mechanisms underlying seasonality of
   person-to-person transmitted diseases are not well understood. Improved
   understanding will enhance understanding of host-pathogen interactions
   and will improve the accuracy of public health surveillance and
   forecasting systems. Insight into seasonal disease patterns may be
   gained through the use of autocorrelation methods or construction of
   periodograms, while seasonal oscillation of infectious diseases can be
   easily simulated using simple transmission models. Models demonstrate
   that small seasonal changes in host or pathogen factors may be
   sufficient to create large seasonal surges in disease incidence, which
   may be important particularly in the context of global climate change.
   Seasonality represents a rich area for future research.}},
